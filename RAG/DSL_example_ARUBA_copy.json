  {
    "title": "Analyze data throughput (Rx and Tx) by AP over last 7 days in 5-min intervals",
    "logic": "The query filters data from site 'HA02' within the last 7 days, calculates throughput metrics for Rx and Tx data for each Wi-Fi access point grouped by 5-minute intervals, applying serial_diff for throughput trends.",
    "content": {
      "summary": "Analyzing Rx and Tx data throughput trends for access points over the last 7 days.",
      "metrics": [
        "max of SSIDTable.RxDataBytes",
        "max of SSIDTable.TxDataBytes",
        "derived Rx throughput (RxDataBytes * 8 / 300)",
        "derived Tx throughput (TxDataBytes * 8 / 300)",
        "serial_diff of Rx throughput",
        "serial_diff of Tx throughput"
      ],
      "filters": [
        "site.keyword = 'HA02'",
        "@timestamp between 'now-7d/d' and 'now/d'"
      ],
      "groupings": [
        "'WirelessTable.AP_Name.keyword' as access point",
        "date_histogram by '@timestamp' in 5-minute fixed intervals"
      ],
      "time_granularity": [
        "5-minute intervals"
      ],
      "methodology": [
        "bucket_script for calculating throughput",
        "serial_diff for analyzing throughput trends"
      ]
    },
    "dsl": {
      "size": 0,
      "query": {
        "bool": {
          "filter": [
            {
              "term": {
                "site.keyword": "HA02"
              }
            },
            {
              "range": {
                "@timestamp": {
                  "gte": "now-7d/d",
                  "lte": "now/d",
                  "format": "strict_date_optional_time"
                }
              }
            }
          ]
        }
      },
      "aggs": {
        "group_by_ap": {
          "terms": {
            "field": "WirelessTable.AP_Name.keyword",
            "size": 100
          },
          "aggs": {
            "time_buckets": {
              "date_histogram": {
                "field": "@timestamp",
                "fixed_interval": "5m",
                "format": "strict_date_optional_time"
              },
              "aggs": {
                "rx_diff": {
                  "max": {
                    "field": "SSIDTable.RxDataBytes"
                  }
                },
                "rx_calculated": {
                  "bucket_script": {
                    "buckets_path": {
                      "max_rx": "rx_diff"
                    },
                    "script": "params.max_rx * 8 / 300"
                  }
                },
                "rx_throughput": {
                  "serial_diff": {
                    "buckets_path": "rx_calculated"
                  }
                },
                "tx_diff": {
                  "max": {
                    "field": "SSIDTable.TxDataBytes"
                  }
                },
                "tx_calculated": {
                  "bucket_script": {
                    "buckets_path": {
                      "max_tx": "tx_diff"
                    },
                    "script": "params.max_tx * 8 / 300"
                  }
                },
                "tx_throughput": {
                  "serial_diff": {
                    "buckets_path": "tx_calculated"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
