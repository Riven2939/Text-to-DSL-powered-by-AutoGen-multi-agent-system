    "logic": "The query filters data within a specified date range and groups results by site and day to calculate the number of unique Wi-Fi users per day for each site.",
    "content": {
      "summary": "Analyzing unique Wi-Fi users per site on a daily basis.",
      "metrics": [
        "cardinality of ClientTable.MACAddress.keyword (unique Wi-Fi users)"
      ],
      "filters": [
        {
          "range": {
            "@timestamp": {
              "gte": "2025-08-20T00:00:00+08:00",
              "lte": "2025-08-27T23:59:59+08:00",
              "format": "strict_date_optional_time"
            }
          }
        }
      ],
      "groupings": [
        "site.keyword",
        "daily intervals based on @timestamp"
      ],
      "time_granularity": [
        "day"
      ],
      "methodology": []
    },
    "dsl": {
      "size": 0,
      "query": {
        "bool": {
          "filter": [
            {
              "range": {
                "@timestamp": {
                  "gte": "2025-08-20T00:00:00+08:00",
                  "lte": "2025-08-27T23:59:59+08:00",
                  "format": "strict_date_optional_time"
                }
              }
            }
          ]
        }
      },
      "aggs": {
        "by_site": {
          "terms": {
            "field": "site.keyword"
          },
          "aggs": {
            "by_day": {
              "date_histogram": {
                "field": "@timestamp",
                "calendar_interval": "day",
                "time_zone": "Asia/Hong_Kong"
              },
              "aggs": {
                "unique_wifi_users": {
                  "cardinality": {
                    "field": "ClientTable.MACAddress.keyword"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
]