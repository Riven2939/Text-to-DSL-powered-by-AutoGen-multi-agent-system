                "field": "@timestamp",
                "fixed_interval": "5m",
                "format": "epoch_millis",
                "min_doc_count": 0,
                "extended_bounds": {
                  "min": 1754469727163,
                  "max": 1754470627163
                }
              },
              "aggs": {
                "available_ap_count": {
                  "cardinality": {
                    "field": "availabilityName.keyword"
                  }
